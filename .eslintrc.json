{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "plugins": ["html"],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "no-var": 0,
        "no-plusplus": 0,
        "indent": [2, 4],
        "no-console": 0,
        "comma-dangle": [2, "always-multiline"],
        "no-param-reassign": [2, { "props": false }],
        "prefer-const": 0,
        "prefer-arrow-callback": 0,
        "func-names": 0,
        "space-before-function-paren": [2, "never"],
        "prefer-template": 0,
        "object-shorthand": 0,
        "new-cap": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "no-unused-expressions": 0,
        "consistent-return": 0     
    },
    "globals": {
        "define": true,                          
        "require": true,                         
        "previewOutCall": true,
        "setLocalStorageLogin": true,
        "arrowFunctions": true,  
        "destructuring": true,
        "restParams": true,  
        "experimentalObjectRestSpread": true
    }
}